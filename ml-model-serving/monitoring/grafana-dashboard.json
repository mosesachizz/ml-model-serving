{
  "dashboard": {
    "id": null,
    "title": "ML Model Serving Dashboard",
    "tags": ["ml", "model-serving", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Prediction Throughput",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(model_predictions_total[5m])",
            "legendFormat": "{{model_version}} - throughput",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests per second"},
          {"format": "short", "show": false}
        ]
      },
      {
        "id": 2,
        "title": "Prediction Latency",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(model_prediction_latency_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{model_version}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(model_prediction_latency_seconds_bucket[5m]))",
            "legendFormat": "p50 - {{model_version}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Seconds"},
          {"format": "short", "show": false}
        ]
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(model_prediction_errors_total[5m]) / rate(model_predictions_total[5m])",
            "legendFormat": "Error rate - {{model_version}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Error rate"},
          {"format": "short", "show": false}
        ]
      },
      {
        "id": 4,
        "title": "Model Versions",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "count by (model_version) (model_predictions_total)",
            "format": "table",
            "refId": "A"
          }
        ],
        "valueName": "current"
      },
      {
        "id": 5,
        "title": "Total Predictions",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 16},
        "targets": [
          {
            "expr": "sum(model_predictions_total)",
            "format": "none",
            "refId": "A"
          }
        ],
        "valueName": "total"
      },
      {
        "id": 6,
        "title": "Success Rate",
        "type": "gauge",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "(1 - (rate(model_prediction_errors_total[5m]) / rate(model_predictions_total[5m]))) * 100",
            "format": "percent",
            "refId": "A"
          }
        ],
        "max": 100,
        "min": 0,
        "thresholds": "80,90"
      }
    ],
    "time": {"from": "now-6h", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "templating": {
      "list": [
        {
          "name": "model_version",
          "type": "query",
          "query": "label_values(model_predictions_total, model_version)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*"
        }
      ]
    },
    "refresh": "10s"
  },
  "overwrite": true
}